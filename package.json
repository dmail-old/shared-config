  "devDependencies": {
    "@dmail/prettiest": "1.0.0",
    "babel-eslint": "8.0.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "5.1.0",
    "eslint-plugin-import": "2.6.0",
    "prettier": "1.9.1",
    "rimraf": "2.6.2",
    "rollup": "0.57.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "2.0.0"
  },
  "scripts": {
    "code-lint": "eslint .",
    "code-format": "prettiest",
    "compile-babel": "node ./bin/build-babel.js",
    "compile-eslint": "node ./bin/build-eslint.js",
    "compile-prettier": "node ./bin/build-prettier.js",
    "code-clean": "rimraf dist",
    "code-compile": "npm run compile-babel && npm run compile-eslint && npm run compile-prettier",
    "code-build": "npm run code-clean && npm run code-compile",
    "compile": "npm run code-compile",
    "build": "npm run code-build",
    "prepublishOnly": "npm run code-compile",
    "ci": "npm run code-compile && npm run code-lint && npm run code-format"
  }
